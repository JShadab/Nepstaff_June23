
public class Test4 {

	public static void main(String[] args) {

		int a = 10;
		int b = 5;

		System.out.println(a < b); // false
		System.out.println(a <= b); // false
		System.out.println(a > b); // true
		System.out.println(a >= b); // true

		int x = 15;

	//	boolean y = (6 < x) & (x > 20); // true & false => false

	}

}
